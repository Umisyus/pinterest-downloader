{
  "extends": "@apify/tsconfig",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "outDir": "dist",
    "noUnusedLocals": false,
    "lib": [
      "DOM",
      "es2015"
    ],
    "skipDefaultLibCheck": true
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
