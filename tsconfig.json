{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "outDir": "dist",
        "noUnusedLocals": false,
        "lib": ["DOM"],
        "skipDefaultLibCheck": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
